nterval\n        timer = setInterval(function()\n        {\n            showRemaining();\n        }, 1000);\n    });\n    <\/script>\n                    <script type=\"text\/x-magento-init\">\n        {\n            \"[data-role=tocart-form], .form.map.checkout\": {\n                \"catalogAddToCart\": {}\n            }\n        }\n        <\/script>\n    \r\n                  <script type=\"text\/javascript\">\r\n                    require([\r\n                      'jquery',\r\n                      'owl.carousel\/owl.carousel.min'\r\n                    ], function ($) {\r\n                      $(\"#featured-products .owl-carousel\").owlCarousel({\r\n                        autoplay: false,\r\n                        autoplayTimeout: 5000,\r\n                        autoplayHoverPause: true,\r\n                        loop: true,\r\n                        navRewind: true,\r\n                        lazyLoad:true,\r\n                        margin: 0,\r\n                        nav: true,\r\n                        dots: false,\r\n                        responsive: {\r\n                          0: {\r\n                            items:2\r\n                          },\r\n                          640: {\r\n                            items:3\r\n                          },\r\n                          768: {\r\n                            items:3\r\n                          },\r\n                          992: {\r\n                            items:4\r\n                          },\r\n                          1200: {\r\n                            items:4\r\n                          }\r\n                        }\r\n                      });\r\n                    });\r\n                  <\/script>\r\n                <\/div>\r\n              <\/div>\r\n            <\/div>\r\n          <\/div>\r\n        <\/div>\r\n      <\/div>\r\n      <div class=\"pt-5\" style=\"background-color: #fbfbfb;\">\r\n        <div class=\"container\">\r\n          <div cla